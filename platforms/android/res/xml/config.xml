<?xml version='1.0' encoding='utf-8'?>
<widget id="com.meu.projetoPhonegap" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FileTransfer">
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
    </feature>
    <name>MeuProjetoPhonegap</name>
    <description>
        Uma aplicação que servirá de base para outras
    </description>
    <author email="sistemas.robson@gmail.com" href="">
        Robson Cesar de Siqueira
    </author>
    <content src="index.html" />
    <icon src="www/res/icon.png" />
    <access origin="*" />
</widget>
